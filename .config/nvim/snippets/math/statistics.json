    "expo dist": {
        "prefix": "expoDist",
        "description": "exponential distribution",
        "body": [
                "x = \\begin{cases}",
                "\t\t\\lambda e^{-\\lambda x} &{x > 0} \\\\\\\\",
                "\t\t0 &{x \\le 0}",
                "\t\\end{cases}"
            ]
    },
    "weibull dist": {
        "prefix": "weibullDist",
        "description": "weibull distribution",
        "body": [
                "x = \\begin{cases}",
                "\t\t\\lambda\\alpha x^{\\alpha-1}\\lambda e^{-\\lambda x^\\alpha} &{x > 0} \\\\\\\\",
                "\t\t0 &{x \\le 0}",
                "\t\\end{cases}"
            ]
    },
    "uniform dist": {
        "prefix": "uniformDist",
        "description": "uniform distribution",
        "body": [
            "x = \\begin{cases}",
            "\t\t\\frac{1}{b-a} &{a \\le x \\le b} \\\\\\",
            "\t\t0 &\\text{less}",
            "\t\\end{cases}"
            ]
    },  
    "multinomial dist": {
        "prefix": "multinomialDist",
